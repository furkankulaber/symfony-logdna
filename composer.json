{
  "name": "furkankulaber/symfony-logdna",
  "type": "symfony-bundle",
  "description": "Integrates LogDna with Symfony",
  "keywords": [
    "symfony",
    "symfony2",
    "symfony3",
    "symfony4",
    "logdna"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Furkan Kulaber"
    }
  ],
  "require": {
    "php": "^5.3.9|^7.0",
    "symfony/dependency-injection": "^2.7|^3.0|^4.0",
    "symfony/event-dispatcher": "^2.7|^3.0|^4.0",
    "symfony/config": "^2.7|^3.0|^4.0",
    "symfony/yaml": "^2.7|^3.0|^4.0",
    "symfony/console": "^2.7|^3.0|^4.0",
    "monolog/monolog": "^2.0"
  },
  "suggest": {
    "symfony/framework-bundle": "To use this lib as a full Symfony Bundle and to use the profiler data collector"
  },
  "autoload": {
    "psr-4": {
      "furkankulaber\\LogDnaBundle\\": ""
    }
  }
}
